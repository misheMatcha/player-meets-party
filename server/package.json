{
	"name": "player-meets-party",
	"version": "1.0.0",
	"description": "OkCupid clone with an emphasis on gaming.",
	"main": "server.js",
	"scripts": {
		"start": "node app.js",
		"server": "nodemon app.js",
		"frontend-install": "npm install --prefix ../client",
		"client": "npm start --prefix ../client",
		"dev": "concurrently \"npm run server\" \"npm run client\"",
		"heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix ../client && npm run build --prefix ../client"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/misheMatcha/player-meets-party.git"
	},
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/misheMatcha/player-meets-party/issues"
	},
	"homepage": "https://github.com/misheMatcha/player-meets-party#readme",
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.19.0",
		"concurrently": "^5.2.0",
		"express": "^4.17.3",
		"express-graphql": "^0.9.0",
		"faker": "^4.1.0",
		"graphql": "^15.0.0",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^5.12.3",
		"passport": "^0.4.1",
		"passport-jwt": "^4.0.0",
		"react-scripts": "^4.0.3",
		"validator": "^13.0.0"
	},
	"devDependencies": {
		"nodemon": "^2.0.4"
	}
}
